Bootstrap: docker
From: ubuntu:18.04

%post
   export DEBIAN_FRONTEND=noninteractive
   # Install common dependencies
   apt-get update
   apt-get --yes install --no-install-recommends \
      python3-dev \
      python3-pip 
   pip3 install setuptools
   pip3 install \
      dash \
      dash-bootstrap-components \
      google-auth-oauthlib \
      ipython
